{
    "root": true,
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": ["standard-with-typescript", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "@typescript-eslint/explicit-function-return-type": 0,
        "@typescript-eslint/strict-boolean-expressions": 0
    }
}
